{
    "name": "td204/silverstripe-custom-admin",
    "description": "Provide a base module to create custom CMS admin sections with full freedom beyond ModelAdmin. Useful for dashboards, overviews, quick settings, task launchers and custom tables.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "cms",
        "admin",
        "custom",
        "dashboard",
        "free"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^6.0",
        "silverstripe/admin": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "squizlabs/php_codesniffer": "^3.7",
        "silverstripe/standards": "^1",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "CustomAdmin\\": "src/",
            "CustomAdmin\\Tests\\": "tests/php/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
